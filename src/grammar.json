{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "athena",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "line"
      }
    },
    "line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "deployment_directive"
        },
        {
          "type": "SYMBOL",
          "name": "version_directive"
        },
        {
          "type": "SYMBOL",
          "name": "section_directive"
        },
        {
          "type": "SYMBOL",
          "name": "service_directive"
        },
        {
          "type": "SYMBOL",
          "name": "service_property"
        },
        {
          "type": "SYMBOL",
          "name": "end_service"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "empty_line"
        }
      ]
    },
    "deployment_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "DEPLOYMENT-ID"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "version_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "VERSION-ID"
        },
        {
          "type": "SYMBOL",
          "name": "version"
        }
      ]
    },
    "section_directive": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "ENVIRONMENT SECTION"
        },
        {
          "type": "STRING",
          "value": "SERVICES SECTION"
        }
      ]
    },
    "service_directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "SERVICE"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "end_service": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "END"
        },
        {
          "type": "STRING",
          "value": "SERVICE"
        }
      ]
    },
    "service_property": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "IMAGE-ID"
            },
            {
              "type": "SYMBOL",
              "name": "value"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "PORT-MAPPING"
            },
            {
              "type": "SYMBOL",
              "name": "port_mapping"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "ENV-VARIABLE"
            },
            {
              "type": "SYMBOL",
              "name": "template_var"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "COMMAND"
            },
            {
              "type": "SYMBOL",
              "name": "quoted_string"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "DEPENDS-ON"
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "HEALTH-CHECK"
            },
            {
              "type": "SYMBOL",
              "name": "quoted_string"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "RESTART-POLICY"
            },
            {
              "type": "SYMBOL",
              "name": "restart_policy"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "RESOURCE-LIMITS"
            },
            {
              "type": "STRING",
              "value": "CPU"
            },
            {
              "type": "SYMBOL",
              "name": "quoted_string"
            },
            {
              "type": "STRING",
              "value": "MEMORY"
            },
            {
              "type": "SYMBOL",
              "name": "quoted_string"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "VOLUME-MAPPING"
            },
            {
              "type": "SYMBOL",
              "name": "quoted_string"
            },
            {
              "type": "STRING",
              "value": "TO"
            },
            {
              "type": "SYMBOL",
              "name": "quoted_string"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "NETWORK-NAME"
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            }
          ]
        }
      ]
    },
    "port_mapping": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": "TO"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "template_var": {
      "type": "PATTERN",
      "value": "\\{\\{[A-Z_][A-Z0-9_]*\\}\\}"
    },
    "quoted_string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "restart_policy": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "always"
        },
        {
          "type": "STRING",
          "value": "unless-stopped"
        },
        {
          "type": "STRING",
          "value": "on-failure"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
    },
    "version": {
      "type": "PATTERN",
      "value": "\\d+\\.\\d+\\.\\d+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "value": {
      "type": "PATTERN",
      "value": "[^\\s]+"
    },
    "comment": {
      "type": "PATTERN",
      "value": "\\/\\/.*"
    },
    "empty_line": {
      "type": "PATTERN",
      "value": "\\s*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}